{
  "name": "ragbookgui",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "concurrently -n backend,frontend -c yellow,cyan \"pnpm run backend:dev\" \"pnpm run frontend:dev\"",
    "frontend:dev": "pnpm --filter frontend dev",
    "frontend:build": "pnpm --filter frontend build",
    "frontend:preview": "pnpm --filter frontend preview",
    "backend:dev": "uvicorn app.main:app --reload --host 0.0.0.0 --port 8000 --app-dir backend"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
